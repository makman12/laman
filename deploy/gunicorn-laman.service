[Unit]
Description=Gunicorn daemon for LAMAN
Requires=gunicorn-laman.socket
After=network.target

[Service]
User=mali
Group=mali
WorkingDirectory=/home/mali/laman
ExecStart=/home/mali/laman/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn/laman.sock \
          laman.wsgi:application

[Install]
WantedBy=multi-user.target
